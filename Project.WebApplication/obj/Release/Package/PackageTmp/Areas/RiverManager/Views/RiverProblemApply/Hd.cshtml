




@{
    ViewBag.Title = "Hd";
    Layout = "~/Views/Shared/_LayoutHandler.cshtml";
}

@section JavaScript{
    <script src="/Areas/RiverManager/Scripts/RiverProblemApply/Hd.js?JsParamter=@SiteConfig.GetConfig().JsParamter"></script>
}
<div style="display: none">
    <input id="BindEntity" type="text" value="@ViewBag.BindEntity" />
    <input id="Command" type="hidden" />
</div>
<div class="mtable_div">
    <form id="form1">
        <table class="mtable">
            @Html.Partial("_RiverDetail")
        </table>
    </form>
    <script type="text/javascript">
        $(function () {
            //$('#RiverId').combobox('enable');  
            //$("#td_div_rivername").html("");
            //    $("#UserCode").css("display","none");
            $("#DepartmentCode").removeAttr("disabled");
            $("#UserCode").removeAttr("disabled");
            $("#Title").removeAttr("disabled");
            $("#ProblemType").removeAttr("disabled");
            $("#Des").removeAttr("disabled");

            $("#ApplyManName").removeAttr("disabled");
            $("#ApplyManTel").removeAttr("disabled");

            $("#tr_piclist").css("display", "none");
            $("#tr_piclist2").css("display", "none");


            $("#btn_RiverChoose").css("display", "");

            }
            );

    </script>
</div>
<div style="padding: 10px 10px 0 10px; text-align: center;">
    @if (Request["PkId"] == null || int.Parse(Request["PkId"]) == 0)
    {
        <a id="btnAdd" class="easyui-linkbutton" iconcls="icon-add" href="javascript:void(0)">新增</a>
    }
    else
    {
        <a id="btnEdit" class="easyui-linkbutton" iconcls="icon-ok" href="javascript:void(0)">编辑</a>
    }
    <a id="btnClose" class="easyui-linkbutton" iconcls="icon-cancel" href="javascript:void(0)">关闭</a>
</div>



