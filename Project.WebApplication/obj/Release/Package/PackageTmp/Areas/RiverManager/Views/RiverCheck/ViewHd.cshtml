




@{
    ViewBag.Title = "Hd";
    Layout = "~/Views/Shared/_LayoutHandler.cshtml";
}

@section JavaScript{
    <script src="/Areas/RiverManager/Scripts/RiverCheck/Hd.js?JsParamter=@SiteConfig.GetConfig().JsParamter"></script>
}
<div style="display: none">
    <input id="BindEntity" type="text" value="@ViewBag.BindEntity" />
    <input id="Command" type="hidden" />
</div>
<div class="mtable_div">
    <form id="form1">
        <table class="mtable">
            <tr id="tr_piclist">
                <td>图片:</td>
                <td colspan="5">
                    <div id="div_img" style="float: left; width: 700px">

                    </div>
                </td>
            </tr>
            <tr>
                <td style="width: 100px">
                    河道名称： <span style="color: Red">*</span>:
                </td>
                <td>
                    <input id="RiverId" name="RiverId" type="text"/>

                </td>

                <td>
                    河长姓名： <span style="color: Red">*</span>:
                </td>
                <td>
                    <input id="UserCode" name="UserCode" type="text"/><textarea style="display: none" id="Coords" name="Coords"></textarea>
                </td>



                <td>
                    备注:
                </td>
                <td>
                    <input id="Remark" name="Remark" type="text"/>
                </td>
            </tr>
           

        </table>

        <iframe name="mapiframe" src="/RiverManager/River/TdMap?command=setPoint&PkId=@Request["PkId"]" frameborder="0" width="100%" height="510px" onload="hiddenButton()"></iframe>
    </form>
</div>
<div style="padding: 10px 10px 0 10px; text-align: center;">
    <a id="btnClose" class="easyui-linkbutton" iconcls="icon-cancel" href="javascript:void(0)">关闭</a>
</div>

<script type="text/javascript">
    function hiddenButton() {
        // alert(11);
        frames["mapiframe"].hiddenButton();
    }

    //$(function() {
    //    $("#mapiframe").on("load", function() {
    //        alert(111);
    //    });
    //});
</script>



