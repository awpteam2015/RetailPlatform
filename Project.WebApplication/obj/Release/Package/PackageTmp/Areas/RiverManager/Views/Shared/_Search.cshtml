
<tr>
    <td>
        问题描述:
    </td>
    <td>
        <input id="Title" name="Title" type="text" />
    </td>

    <td>
        问题类型 :
    </td>
    <td>

        <select id="ProblemType" name="ProblemType">
            <option value="0">请选择</option>
            <option value="1">日常巡河</option>
            <option value="2">问题上报</option>
            <option value="3">群众举报</option>
        </select>

    </td>

    <td>
        所属部门:
    </td>
    <td>
        <input id="DepartmentCode" name="DepartmentCode" type="text" style="width: 200px" />
    </td>

    <td>
        河道名称 :
    </td>
    <td>
        <input id="RiverName" name="RiverName" type="text" />
    </td>
</tr>

<tr>
    <td>
        河长手机 :
    </td>
    <td>
        <input id="UserCode" name="UserCode" type="text" />
    </td>

    <td>
        河长姓名:
    </td>
    <td>
        <input id="UserName" name="UserName" type="text" />
    </td>

    <td>
        问题状态：
    </td>
    <td>

        <select id="State" name="State">
            <option value="">请选择</option>
            <option value="1">部门待处理</option>
            <option value="2">河长待处理</option>
            <option value="3">完结</option>
            <option value="4">重新申请作废</option>
            <option value="5">回退部门待处理</option>
        </select>
    </td>

    <td>
        是否删除：
    </td>
    <td>

        <select id="IsDeleted" name="IsDeleted">
            <option value="0">否</option>
            <option value="1">是</option>
        </select>
    </td>

</tr>

