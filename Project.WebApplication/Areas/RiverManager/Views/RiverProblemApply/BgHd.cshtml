




@{
    ViewBag.Title = "Hd";
    Layout = "~/Views/Shared/_LayoutHandler.cshtml";
}

@section JavaScript{
    <script src="/Areas/RiverManager/Scripts/RiverProblemApply/RiverProblemHd.js?JsParamter=@SiteConfig.GetConfig().JsParamter"></script>
}
<div style="display: none">
    <input id="BindEntity" type="text" value="@ViewBag.BindEntity" />
    <input id="Command" type="hidden" />
</div>
<div class="mtable_div">
    <form id="form1">
        <table class="mtable">
            @Html.Partial("_RiverDetail")
            <tr>
                <td>
                    是否曝光
                </td>
                <td>
                    <select id="IsExposure" name="IsExposure">
                        <option value="1">是</option>
                        <option value="0">否</option>

                    </select>
                </td>
            </tr>
            <tr>
                <td>
                    曝光等级:
                </td>
                <td>
                    <select id="ExposureLever" name="ExposureLever">
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td>
                    曝光范围:
                </td>
                <td>
                    <input id="ExposureArea" name="ExposureArea"/>
                  
                </td>
            </tr>
        </table>
    </form>
</div>
<div style="padding: 10px 10px 0 10px; text-align: center;">
    <a id="btnEdit" class="easyui-linkbutton" iconcls="icon-ok" href="javascript:void(0)">确认</a>
    <a id="btnClose" class="easyui-linkbutton" iconcls="icon-cancel" href="javascript:void(0)">关闭</a>
</div>



