

  


@{
    ViewBag.Title = "Hd";
    Layout = "~/Views/Shared/_LayoutHandler.cshtml";
}

@section JavaScript{
    <script src="/Areas/CustomerManager/Scripts/Customer/Hd.js?JsParamter=@SiteConfig.GetConfig().JsParamter"></script>
}
<div style="display: none">
<input id="BindEntity" type="text" value="@ViewBag.BindEntity" />
<input id="Command" type="hidden" />
</div>
<div class="mtable_div">
    <form id="form1">
        <table class="mtable">
            
              <tr>
            <td  >
                <span style="color: Red">*</span>:
            </td>
            <td >
                <input id="PkId" name="PkId"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                <span style="color: Red">*</span>:
            </td>
            <td >
                <input id="CardNo" name="CardNo"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                密码<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Password" name="Password"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                会员名称<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="CustomerName" name="CustomerName"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                性别<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Gender" name="Gender"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                生日<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Birthday" name="Birthday"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                邮件<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Email" name="Email"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                家庭电话<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Familytelephone" name="Familytelephone"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                邮编<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Postcode" name="Postcode"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                手机<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Mobilephone" name="Mobilephone"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                居住地址   省<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="ProvinceId" name="ProvinceId"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                居住地址   市<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="CityId" name="CityId"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                居住地址   区（新增）<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="CountryId" name="CountryId"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                居住地址   详细地址<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Address" name="Address"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                备注<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Memo" name="Memo"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                折扣率<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Discount" name="Discount"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                消费总金额<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Totalamount" name="Totalamount"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                总积分<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Totalpoints" name="Totalpoints"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                可用积分<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="Availablepoints" name="Availablepoints"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                修改时间<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="LastModificationTime" name="LastModificationTime"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                修改人<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="LastModifierUserCode" name="LastModifierUserCode"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                创建时间<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="CreationTime" name="CreationTime"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                创建人<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="CreatorUserCode" name="CreatorUserCode"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                是否删除<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="IsDeleted" name="IsDeleted"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                删除人<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="DeleterUserCode" name="DeleterUserCode"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td  >
                删除时间<span style="color: Red">*</span>:
            </td>
            <td >
                <input id="DeletionTime" name="DeletionTime"  type="text"  />
            </td>
        </tr>
          
        </table>
    </form>
</div>
<div style="padding: 10px 10px 0 10px; text-align: center;">
    @if (Request["PkId"]==null||int.Parse(Request["PkId"]) == 0)
    {
        <a id="btnAdd" class="easyui-linkbutton" iconcls="icon-add" href="javascript:void(0)">新增</a>
    }
    else
    {
        <a id="btnEdit" class="easyui-linkbutton" iconcls="icon-ok" href="javascript:void(0)">编辑</a>
    }
    <a id="btnClose" class="easyui-linkbutton" iconcls="icon-cancel" href="javascript:void(0)">关闭</a>
</div>





