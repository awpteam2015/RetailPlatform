

  



@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_LayoutList.cshtml";
}

@section JavaScript{
    <script src="/Areas/CustomerManager/Scripts/CustomerAddress/List.js?JsParamter=@SiteConfig.GetConfig().JsParamter" type="text/javascript"></script>
}

<div title="送货地址簿" style="padding:1px;">
    <table id="datagrid" style="height:auto;" iconcls=".icon-save" singleselect="true"  toolbar="#tb" fit="true"></table>
    <div id="tb" style="padding:5px;height:auto">
        <div  style="margin-bottom: 5px">
            <a id="btnAdd" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true" >添加</a>
  
            <a id="btnDel" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-remove',plain:true" >删除</a>
           
            <a id="btnEdit" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-edit',plain:true" >编辑</a>
          
            <a id="btnRefresh" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-reload',plain:true" >刷新</a>
           
        </div>
      
        <div id="divSearch">
            <form>
            
             <table class="mtable">
            
              <tr>
            <td >
                自动增加得建立序列:
            </td>
            <td >
                <input id="PkId" name="PkId"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                :
            </td>
            <td >
                <input id="CustomerId" name="CustomerId"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                送货地址  省:
            </td>
            <td >
                <input id="Province" name="Province"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                送货地址   市:
            </td>
            <td >
                <input id="CityId" name="CityId"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                送货地址   区（新增）:
            </td>
            <td >
                <input id="CountryId" name="CountryId"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                送货地址   详细地址:
            </td>
            <td >
                <input id="Address" name="Address"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                是否是默认地址:
            </td>
            <td >
                <input id="IsDefault" name="IsDefault"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                备注:
            </td>
            <td >
                <input id="Remarks" name="Remarks"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                收货人姓名:
            </td>
            <td >
                <input id="ReceiverName" name="ReceiverName"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                电话:
            </td>
            <td >
                <input id="FamilyTelephone" name="FamilyTelephone"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                邮编:
            </td>
            <td >
                <input id="PostCode" name="PostCode"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                手机:
            </td>
            <td >
                <input id="Mobilephone" name="Mobilephone"  type="text"  />
            </td>
        </tr>
          <tr>
        <td colspan="2">   <a href="#" class="easyui-linkbutton" iconcls="icon-search" id="btnSearch">搜索</a>
        </td>
        </tr>
        </table>
            </form>
        </div>
    </div>
</div>





