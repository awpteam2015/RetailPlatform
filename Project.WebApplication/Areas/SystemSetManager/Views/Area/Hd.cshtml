




@{
    ViewBag.Title = "Hd";
    Layout = "~/Views/Shared/_LayoutHandler.cshtml";
}

@section JavaScript{
    <script src="/Areas/SystemSetManager/Scripts/Area/Hd.js?JsParamter=@SiteConfig.GetConfig().JsParamter"></script>
}
<div style="display: none">
    <input id="BindEntity" type="text" value="@ViewBag.BindEntity" />
    <input id="Command" type="hidden" />
</div>
<div class="mtable_div">
    <form id="form1">
        <table class="mtable">
            <tr>
                <td>
                    区编码 <span style="color: Red">*</span>:
                </td>
                <td>
                    <input id="AreaId" name="AreaId" type="text" />
                </td>
            </tr>
            <tr>
                <td>
                    区名称 <span style="color: Red">*</span>:
                </td>
                <td>
                    <input id="Area" name="Area" type="text" />
                </td>
            </tr>
            <tr>
                <td>
                    市编码<span style="color: Red">*</span>:
                </td>
                <td>
                    <input id="CityId" name="CityId" type="text" />
                </td>
            </tr>

            <tr>
                <td>
                    首重 <span style="color: Red">*</span>:
                </td>
                <td>
                    <input id="FirstWeightPrice" name="FirstWeightPrice" type="text" />
                </td>
            </tr>
            <tr>
                <td>
                    续重  <span style="color: Red">*</span>:
                </td>
                <td>
                    <input id="SecondWeightPrice" name="SecondWeightPrice" type="text" />
                </td>
            </tr>
        </table>
    </form>
</div>
<div style="padding: 10px 10px 0 10px; text-align: center;">
    @if (Request["PkId"] == null || int.Parse(Request["PkId"]) == 0)
    {
        <a id="btnAdd" class="easyui-linkbutton" iconcls="icon-add" href="javascript:void(0)">新增</a>
    }
    else
    {
        <a id="btnEdit" class="easyui-linkbutton" iconcls="icon-ok" href="javascript:void(0)">编辑</a>
    }
    <a id="btnClose" class="easyui-linkbutton" iconcls="icon-cancel" href="javascript:void(0)">关闭</a>
</div>



