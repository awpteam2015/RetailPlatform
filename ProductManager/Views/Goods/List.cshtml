

  



@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_LayoutList.cshtml";
}

@section JavaScript{
    <script src="/Areas/ProductManager/Scripts/Goods/List.js?JsParamter=@SiteConfig.GetConfig().JsParamter" type="text/javascript"></script>
}

<div title="商品表" style="padding:1px;">
    <table id="datagrid" style="height:auto;" iconcls=".icon-save" singleselect="true"  toolbar="#tb" fit="true"></table>
    <div id="tb" style="padding:5px;height:auto">
        <div  style="margin-bottom: 5px">
            <a id="btnAdd" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true" >添加</a>
  
            <a id="btnDel" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-remove',plain:true" >删除</a>
           
            <a id="btnEdit" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-edit',plain:true" >编辑</a>
          
            <a id="btnRefresh" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-reload',plain:true" >刷新</a>
           
        </div>
      
        <div id="divSearch">
            <form>
            
             <table class="mtable">
            
              <tr>
            <td >
                :
            </td>
            <td >
                <input id="PkId" name="PkId"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                :
            </td>
            <td >
                <input id="ProductId" name="ProductId"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                货号:
            </td>
            <td >
                <input id="GoodsCode" name="GoodsCode"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                库存:
            </td>
            <td >
                <input id="GoodsStock" name="GoodsStock"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                销售价:
            </td>
            <td >
                <input id="GoodsPrice" name="GoodsPrice"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                成本价:
            </td>
            <td >
                <input id="GoodsCost" name="GoodsCost"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                重量:
            </td>
            <td >
                <input id="GoodsWeight" name="GoodsWeight"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                重量单位:
            </td>
            <td >
                <input id="GoodsWeightUnit" name="GoodsWeightUnit"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                单位:
            </td>
            <td >
                <input id="Unit" name="Unit"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                商品描述:
            </td>
            <td >
                <input id="Title" name="Title"  type="text"  />
            </td>
        </tr>
            
              <tr>
            <td >
                是否是默认商品:
            </td>
            <td >
                <input id="IsDefault" name="IsDefault"  type="text"  />
            </td>
        </tr>
          <tr>
        <td colspan="2">   <a href="#" class="easyui-linkbutton" iconcls="icon-search" id="btnSearch">搜索</a>
        </td>
        </tr>
        </table>
            </form>
        </div>
    </div>
</div>





